{
  "name": "@ben_12/eslint-plugin-dprint",
  "version": "0.7.0",
  "description": "An ESLint plugin that fixes code with dprint",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/lib/index.js",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "eslint": ">=5.16.0"
  },
  "dependencies": {
    "@dprint/typescript": "^0.88.1",
    "debug": "^4.1.1",
    "diff": "^5.0.0",
    "jsonc-parser": "^3.2.0"
  },
  "devDependencies": {
    "@ben_12/eslint-plugin-dprint": "^0.6.1",
    "@types/debug": "^4.1.5",
    "@types/diff": "^5.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/utils": "^6.0.0",
    "eslint": "^8.50.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.1",
    "rimraf": "^5.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.0.0",
    "typescript": "~5.0.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "codecov": "nyc report --reporter text-lcov | codecov --pipe --disable=gcov",
    "lint": "eslint --rulesdir scripts/internal-rules \"**/*.ts\"",
    "test": "npm run -s lint && npm run -s test:ci",
    "test:ci": "tsc && nyc mocha \"test/rules/**/*.ts\"",
    "watch": "mocha --require ts-node/register --watch --watch-files \"lib/**/*.ts,test/**/*.ts\" \"test/rules/**/*.ts\"",
    "preversion": "npm test",
    "version": "npm run -s build",
    "postversion": "ts-node scripts/release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben12/eslint-plugin-dprint.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "dprint"
  ],
  "author": "Beno√Æt Moreau <public@ben12.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ben12/eslint-plugin-dprint/issues"
  },
  "homepage": "https://eslint-plugin-dprint.ben12.eu"
}
