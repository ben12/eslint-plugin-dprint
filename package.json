{
  "name": "@ben_12/eslint-plugin-dprint",
  "version": "1.0.0",
  "description": "An ESLint plugin that fixes code with dprint",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/lib/index.js",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@dprint/dockerfile": "^0.3.2",
    "@dprint/json": "^0.19.4",
    "@dprint/markdown": "^0.17.8",
    "@dprint/toml": "^0.6.3",
    "@dprint/typescript": "^0.93.3",
    "dprint-plugin-malva": "^0.11.1",
    "dprint-plugin-markup": "^0.18.0",
    "eslint": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "@dprint/dockerfile": {
      "optional": true
    },
    "@dprint/json": {
      "optional": true
    },
    "@dprint/markdown": {
      "optional": true
    },
    "@dprint/toml": {
      "optional": true
    },
    "@dprint/typescript": {
      "optional": true
    },
    "dprint-plugin-malva": {
      "optional": true
    },
    "dprint-plugin-markup": {
      "optional": true
    }
  },
  "dependencies": {
    "@dprint/formatter": "^0.2.0",
    "debug": "^4.1.1",
    "diff": "^5.0.0",
    "jsonc-parser": "^3.2.0"
  },
  "devDependencies": {
    "@ben_12/eslint-plugin-dprint": "file:./test/internal-rules",
    "@ben_12/eslint-simple-parser": "^0.1.0",
    "@types/debug": "^4.1.5",
    "@types/diff": "^5.0.0",
    "@types/eslint": "^9.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "axios": "^1.7.9",
    "eslint": "^9.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.1",
    "rimraf": "^5.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.0.0",
    "typescript": "~5.0.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "codecov": "nyc report --reporter text-lcov | codecov --pipe --disable=gcov",
    "lint": "eslint \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "test": "npm run -s lint && npm run -s test:ci",
    "test:ci:7.x": "tsc && nyc mocha \"test/rules/**/*.ts\"",
    "test:ci:8.x": "tsc && nyc mocha \"test/rules/**/*.ts\"",
    "test:ci:9.x": "tsc && nyc mocha \"test/rules/**/*.ts\"",
    "test:ci": "tsc && nyc mocha \"test/rules/**/*.ts\"",
    "watch": "mocha --require ts-node/register --watch --watch-files \"lib/**/*.ts,test/**/*.ts\" \"test/rules/**/*.ts\"",
    "preversion": "npm test",
    "version": "npm run -s build",
    "postversion": "ts-node scripts/release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben12/eslint-plugin-dprint.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "dprint",
    "formatter"
  ],
  "author": "Beno√Æt Moreau <public@ben12.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ben12/eslint-plugin-dprint/issues"
  },
  "homepage": "https://eslint-plugin-dprint.ben12.eu"
}
